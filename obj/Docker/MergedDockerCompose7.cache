networks:
  backend: {}
  frontend: {}
services:
  eventshubcatalogapi:
    build:
      context: F:\Cohort\EventsHub\EventsHubCatalogAPI
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EventsHubCatalogAPI
      target: base
    container_name: eventshubcatalogapi
    depends_on:
      mssqlserver:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionString: Data Source=mssqlserver;Initial Catalog=EventsHubDb;User Id=sa;Password=ProductApi123(!);Connect
        Timeout=30;TrustServerCertificate=false;Encrypt=false;
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      ExternalBaseUrl: http://localhost:7024
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: eventshub/catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net6.0/EventsHubCatalogAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 7024
      target: 80
    tty: true
    volumes:
    - F:\Cohort\EventsHub\EventsHubCatalogAPI:/app:rw
    - C:\Users\sergey\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\sergey\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\sergey\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\sergey\.nuget\packages:/root/.nuget/packages:ro
    - F:\Cohort\EventsHub\EventsHubCatalogAPI:/src:rw
  mssqlserver:
    container_name: eventshubsqlserver
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: ProductApi123(!)
      SA_PASSWORD: ProductApi123(!)
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      backend: {}
    ports:
    - published: 1450
      target: 1433
  webmvc:
    build:
      context: F:\Cohort\EventsHub\WebMvc
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebMvc
      target: base
    container_name: eventswebshop
    depends_on:
      eventshubcatalogapi:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CatalogUrl: http://eventshubcatalogapi
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: eventshub/webmvc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net6.0/WebMvc.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      frontend: {}
    ports:
    - published: 7501
      target: 80
    tty: true
    volumes:
    - F:\Cohort\EventsHub\WebMvc:/app:rw
    - C:\Users\sergey\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\sergey\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\sergey\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\sergey\.nuget\packages:/root/.nuget/packages:ro
    - F:\Cohort\EventsHub\WebMvc:/src:rw
version: '3.4'