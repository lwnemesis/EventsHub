name: dockercompose15803436057179011628
services:
  eventshubcatalogapi:
    build:
      context: D:\Dev\EventsHub\EventsHubCatalogAPI
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EventsHubCatalogAPI
      target: base
    container_name: eventshubcatalogapi
    depends_on:
      mssqlserver:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionString: Data Source=mssqlserver;Initial Catalog=EventsHubDb;User Id=sa;Password=ProductApi123(!);Connect
        Timeout=30;TrustServerCertificate=false;Encrypt=false;
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      ExternalBaseUrl: http://localhost:7024
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: eventshub/catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/EventsHubCatalogAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: null
      frontend: null
    ports:
    - mode: ingress
      target: 80
      published: "7024"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Dev\EventsHub\EventsHubCatalogAPI
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Dev\EventsHub\EventsHubCatalogAPI
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  mssqlserver:
    container_name: eventshubsqlserver
    environment:
      ACCEPT_EULA: "true"
      MSSQL_SA_PASSWORD: ProductApi123(!)
      SA_PASSWORD: ProductApi123(!)
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 1433
      published: "1450"
      protocol: tcp
  webmvc:
    build:
      context: D:\Dev\EventsHub\WebMvc
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebMvc
      target: base
    container_name: eventswebshop
    depends_on:
      eventshubcatalogapi:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CatalogUrl: http://eventshubcatalogapi
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: eventshub/webmvc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/WebMvc.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      frontend: null
    ports:
    - mode: ingress
      target: 80
      published: "7501"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Dev\EventsHub\WebMvc
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Dev\EventsHub\WebMvc
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\David M\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
