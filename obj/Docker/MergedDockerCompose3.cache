networks:
  backend: {}
  frontend: {}
services:
  basket.data:
    container_name: eventshubbasketdata
    image: redis
    networks:
      backend: {}
    ports:
    - published: 7051
      target: 6379
  cart:
    build:
      context: F:\Cohort\Copy Of\EventsHub\CartAPI
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CartAPI
      target: base
    container_name: eventshubcartapi
    depends_on:
      basket.data:
        condition: service_started
      tokenserver:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionString: basket.data
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      IdentityUrl: http://192.168.168.67:7801
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: eventshub/cartapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/net6.0/CartAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: {}
      frontend: {}
    ports:
    - published: 7011
      target: 80
    tty: true
    volumes:
    - F:\Cohort\Copy Of\EventsHub\CartAPI:/app:rw
    - C:\Users\sergey\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\sergey\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\sergey\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
